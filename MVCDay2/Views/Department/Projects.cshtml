@{
    Layout = "_Layout";
}

@foreach (var project in Model)
{

    <ul>
        <li>Name: @project.Name</li>
        <li>Location: @project.Location</li>
        <li><a asp-action="AddEmpProj" asp-controller="Department" asp-route-id="@project.Dept_id" asp-route-Pnum="@project.Number">AddEmployee</a></li>
        <li><a asp-action="Edit" asp-controller="Department" asp-route-id="@project.Number">Edit</a></li>
        <li><a asp-action="Delete" asp-controller="Department" asp-route-id="@project.Number">Delete</a></li>
    </ul>
    }

