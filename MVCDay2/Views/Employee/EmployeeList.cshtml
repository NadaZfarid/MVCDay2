@{
    Layout = "_Layout";
}

<h1> Employees : </h1>

        @foreach (var employee in Model)
        {
            <ul>
                <li>Fname: @employee.Fname</li>
                <li>Lname: @employee.LName</li>
                <li>Address: @employee.Address</li>
                <li>Gender: @employee.Gender</li>
                <li>BDate: @employee.BDate</li>
                <li>Salary: @employee.Salary</li>
                <li><a href="/Employee/Edit/@employee.SSN">Edit</a></li>
                <li><a href="/Employee/Delete/@employee.SSN">Delete</a></li>
            </ul><hr />
        }
    

<a href="/Employee/Add">Add Employee</a>
